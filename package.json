{
  "name": "tennico",
  "version": "0.1.0",
  "description": "The application to find tennis court",
  "engines": {
    "node": "16"
  },
  "scripts": {
    "dev": "next src/app",
    "preserve": "npm run build-public && npm run build-functions && npm run build-app && npm run copy-deps && npm run install-deps",
    "serve": "cross-env NODE_ENV=production firebase serve",
    "deploy": "firebase deploy",
    "clean": "rimraf \"dist/functions\" && rimraf \"dist/public\"",
    "build-app": "next build \"src/app\"",
    "build-public": "cpx \"src/app/public/**/*.*\" \"dist/public\" -C",
    "build-functions": "tsc --project src/functions",
    "lint-app": "eslint src/app --ext .ts,.tsx",
    "typecheck-app": "tsc --project src/app",
    "lint-functions": "eslint src/functions --ext .ts,.tsx",
    "copy-deps": "cpx \"*{package.json,package-lock.json,yarn.lock}\" \"dist/functions\" -C",
    "install-deps": "cd \"dist/functions\" && npm i",
    "test": "firebase emulators:exec --only firestore 'jest --silent'"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@googlemaps/google-maps-services-js": "^3.3.38",
    "@mui/icons-material": "^5.14.11",
    "@mui/material": "^5.14.11",
    "@react-google-maps/api": "^2.19.2",
    "@reduxjs/toolkit": "^1.9.6",
    "@sentry/browser": "^7.72.0",
    "@types/react": "^18.2.23",
    "algoliasearch": "^4.20.0",
    "firebase": "^10.4.0",
    "firebase-admin": "^11.11.0",
    "firebase-functions": "^4.4.1",
    "firebaseui": "^6.1.0",
    "formik": "^2.4.5",
    "moment": "^2.29.4",
    "next": "^13.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "redux": "^4.2.1",
    "sitemap": "^7.1.1",
    "yup": "^1.3.1"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^3.0.1",
    "@types/jest": "^29.5.5",
    "@types/node": "20.7.1",
    "@types/react-dom": "^18.2.8",
    "@types/react-redux": "^7.1.27",
    "@types/webpack": "^5.28.3",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "cpx": "1.5.0",
    "cross-env": "7.0.3",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "firebase-functions-test": "^3.1.0",
    "firebase-tools": "^12.6.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "rimraf": "5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "resolutions": {
    "@types/react": "^16.13.1"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  }
}
