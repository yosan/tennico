{
  "name": "tennico",
  "version": "0.1.0",
  "description": "The application to find tennis court",
  "engines": {
    "node": "16"
  },
  "scripts": {
    "dev": "next src/app",
    "preserve": "npm run build-public && npm run build-functions && npm run build-app && npm run copy-deps && npm run install-deps",
    "serve": "cross-env NODE_ENV=production firebase serve",
    "deploy": "firebase deploy",
    "clean": "rimraf \"dist/functions\" && rimraf \"dist/public\"",
    "build-app": "next build \"src/app\"",
    "build-public": "cpx \"src/app/public/**/*.*\" \"dist/public\" -C",
    "build-functions": "tsc --project src/functions",
    "lint-app": "eslint src/app --ext .ts,.tsx",
    "typecheck-app": "tsc --project src/app",
    "lint-functions": "eslint src/functions --ext .ts,.tsx",
    "copy-deps": "cpx \"*{package.json,package-lock.json,yarn.lock}\" \"dist/functions\" -C",
    "install-deps": "cd \"dist/functions\" && npm i",
    "test": "firebase emulators:exec --only firestore 'jest --silent'"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@googlemaps/google-maps-services-js": "^3.3.26",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.4",
    "@react-google-maps/api": "^2.17.1",
    "@reduxjs/toolkit": "^1.9.1",
    "@sentry/browser": "^7.29.0",
    "algoliasearch": "^4.14.3",
    "firebase": "^9.15.0",
    "firebase-admin": "^11.4.1",
    "firebase-functions": "^4.1.1",
    "firebaseui": "^6.0.2",
    "formik": "^2.2.6",
    "moment": "^2.29.4",
    "next": "^13.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.0",
    "sitemap": "^7.1.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^2.0.5",
    "@types/jest": "^29.2.5",
    "@types/node": "18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-redux": "^7.1.25",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "cpx": "1.5.0",
    "cross-env": "7.0.3",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "firebase-functions-test": "^3.0.0",
    "firebase-tools": "^11.20.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.2",
    "rimraf": "3.0.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  },
  "resolutions": {
    "@types/react": "^16.13.1"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  }
}
