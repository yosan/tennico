{
  "name": "tennico",
  "version": "0.1.0",
  "description": "The application to find tennis court",
  "engines": {
    "node": "14"
  },
  "scripts": {
    "dev": "next src/app",
    "preserve": "npm run build-public && npm run build-functions && npm run build-app && npm run copy-deps && npm run install-deps",
    "serve": "cross-env NODE_ENV=production firebase serve",
    "deploy": "firebase deploy",
    "clean": "rimraf \"dist/functions\" && rimraf \"dist/public\"",
    "build-app": "next build \"src/app\"",
    "build-public": "cpx \"src/app/public/**/*.*\" \"dist/public\" -C",
    "build-functions": "tsc --project src/functions",
    "lint-app": "eslint src/app --ext .ts,.tsx",
    "typecheck-app": "tsc --project src/app",
    "lint-functions": "eslint src/functions --ext .ts,.tsx",
    "copy-deps": "cpx \"*{package.json,package-lock.json,yarn.lock}\" \"dist/functions\" -C",
    "install-deps": "cd \"dist/functions\" && npm i",
    "test": "firebase emulators:exec --only firestore 'jest --silent'"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.1.16",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@react-google-maps/api": "^2.1.1",
    "@reduxjs/toolkit": "^1.5.1",
    "@sentry/browser": "^5.24.2",
    "@types/webpack": "^4.41.22",
    "@types/yup": "^0.29.11",
    "algoliasearch": "^4.5.1",
    "firebase": "^8.2.2",
    "firebase-admin": "^9.4.2",
    "firebase-functions": "^3.13.0",
    "firebaseui": "^4.7.1",
    "formik": "^2.2.6",
    "moment": "^2.29.4",
    "next": "^12.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-redux-firebase": "^3.10.0",
    "redux": "^4.0.4",
    "redux-firestore": "^0.15.0",
    "sitemap": "^6.3.2",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^1.1.10",
    "@types/jest": "^26.0.20",
    "@types/node": "14.11.2",
    "@types/react": "^16.9.50",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.16",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "cpx": "1.5.0",
    "cross-env": "7.0.2",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "firebase-functions-test": "^0.2.3",
    "firebase-tools": "^9.9.0",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "rimraf": "3.0.2",
    "ts-jest": "^26.5.0",
    "typescript": "^4.0.3"
  },
  "resolutions": {
    "@types/react": "^16.13.1"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  }
}
